{
  "env": {
    "production": {
      "plugins": [
        [ "emotion", { "hoist": true } ]
      ]
    },
    "development": {
      "plugins": [
        [ "emotion", { "sourceMap": true, "autoLabel": true } ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-dynamic-import-node"
  ],
  "presets": [
    [
      "@babel/preset-env", {
        "useBuiltIns": "usage",
        "modules": false
      }
    ],
    "@babel/preset-react"
  ]
}
