{
  "parser": "babel-eslint",
  "plugins": [ "html" ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-var": [1],
    "new-cap": [1],
    "no-console": [0],
    "dot-notation": [1],
    "no-unused-vars": [1],
    "react/prop-types": [0],
    "indent": [2, 2, { "SwitchCase": 1 }]
  },
  "globals": {
    "window": true,
    "document": true
  }
}
